<svg width="20" height="1080">

<defs>
<linearGradient id="blue_lightblue" x1="0%" y1="0%" x2="0%" y2="100%">
<stop offset="0%" style="stop-color:rgb(123,146,175);stop-opacity:1;"/>
<stop offset="100%" style="stop-color:rgb(0,86,146);stop-opacity:1;"/>
</linearGradient>
<linearGradient id="grey_darkgrey" x1="0%" y1="0%" x2="0%" y2="100%">
<stop offset="0%" style="stop-color:rgb(160,160,160);stop-opacity:1;"/>
<stop offset="100%" style="stop-color:rgb(70,70,70);stop-opacity:1;"/>
</linearGradient>
<linearGradient id="lightgrey_grey" x1="0%" y1="0%" x2="0%" y2="100%">
<stop offset="0%" style="stop-color:#F1F1F1;stop-opacity:1;"/>
<stop offset="100%" style="stop-color:rgb(160,160,160);stop-opacity:1;"/>
</linearGradient>
<linearGradient id="topshadowgradient" x1="0%" y1="0%" x2="0%" y2="100%">
