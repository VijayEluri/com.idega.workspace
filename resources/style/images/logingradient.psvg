<stop offset="25%" style="stop-color:rgb(0,0,0);stop-opacity:0.25;"/>
<stop offset="50%" style="stop-color:rgb(0,0,0);stop-opacity:0.12;"/>
<stop offset="75%" style="stop-color:rgb(0,0,0);stop-opacity:0.05;"/>
<stop offset="100%" style="stop-color:rgb(0,0,0);stop-opacity:0;"/>
</linearGradient>

<pattern id="pinstripePattern" patternUnits="userSpaceOnUse" x="0" y="0" width="3" height="3" viewBox="0 0 3 3">
<line x1="-1" y1="2" x2="4" y2="2" style="stroke:#AAAAAA; stroke-width:1; opacity:0.3;"/>
</pattern>

</defs>

<rect width="20" height="1080" x="0" y="0" style="fill:url(#lightgrey_grey)" />
<rect width="20" height="1080" x="0" y="0" style="fill:url(#pinstripePattern)" />
<rect width="20" height="30" x="0" y="0" style="fill:url(#topshadowgradient)"/>

</svg>
